{
  "name": "Basic shed door",
  "type": "Door",
  "subtype": "shed",
  "description": "Basic vertical wood door with etched hinges on right of door",
  "comments": "Shed door etch on 3 sides as bottom is already cut. Door outside are in outers which will be cut or etch based on settings. Texture is manually included int he etches (first 4 entries). Hinges are from  right hand side.",
  "defaults": 
    {
      "width": 800, 
      "height": 1800
    },
  "typical": 
    {
      "hingewidth": 300,
      "hingeheight": 50,
      "woodetch" : 10,
      "woodheight" : 150
    }
  "outers":
    [
        ["line", [["x", "y + height"], ["x", "y"]]],
        ["line", [["x", "y"], ["x + width", "y"]]],
        ["line", [["x + width", "y"], ["x + width", "y = height"]]]
    ]
  "cuts": 
    [

    ],
  "etches":
    [
        ["rect", [["x + 175", "y"], ["woodetch", "height"]]],
        ["rect", [["x + 325", "y"], ["woodetch", "height"]]],
        ["rect", [["x + 475", "y"], ["woodetch", "height"]]],
        ["rect", [["x + 625", "y"], ["woodetch", "height"]]],
        ["polygon", [
            ["x + 800", "y + (height*0.15)"],
            ["x + 800 - hingewidth", "y + (height*0.15) + (hingeheight/2)"],
            ["x + 800", "y + (height*0.15) + (hingeheight)"]
        ]],
        ["polygon", [
            ["x + 800", "y + (height*0.5)"],
            ["x + 800 - hingewidth", "y + (height*0.5) + (hingeheight/2)"],
            ["x + 800", "y + (height*0.5) + (hingeheight)"]
        ]],
        ["polygon", [
            ["x + 800", "y + (height*0.85)"],
            ["x + 800 - hingewidth", "y + (height*0.85) + (hingeheight/2)"],
            ["x + 800", "y + (height*0.85) + (hingeheight)"]
        ]]            
    ]

}